}

void loop() { 


// Put Qbert first frame
for(int passtime = 0; passtime < 8; passtime++) { // Display it 8 times

FastLED.clear();

for(int h = 0; h < ANIM8SIZE; h++) {
  for(int i = 0; i < 1; i++) {
